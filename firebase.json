{
  "firebase": "quesearme",
  "public": ".",
  
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "redirects": [ {
	"source" : "/foo",
	"destination" : "/bar",
	"type" : 301
	}, {
	"source" : "/firebase/*",
	"destination" : "https://www.firebase.com",
	"type" : 302
} ]
}

